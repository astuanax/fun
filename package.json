{
  "name": "fun.js",
  "sideEffects": false,
  "version": "1.0.5",
  "description": "Functional tools for javascript development",
  "main": "lib/fun.js",
  "module": "src/index.js",
  "scripts": {
    "build": "./node_modules/.bin/eslint ./src --fix && npm run compile && npm run test && npm run jsdocs-json && npm run jsdocs && npm run size",
    "compile": "webpack --env dev && webpack --env build",
    "index": "npx create-index -r ./src && npx create-index ./src",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --require @babel/register --colors ./test/*.spec.js",
    "test:watch": "mocha --require @babel/register --colors -w ./test/*.spec.js",
    "test:cover": "cross-env NODE_ENV=test nyc mocha --require babel-register --colors test/*.spec.js",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "repl": "node -i -e \"$(< ./lib/fun.js)\"",
    "size": "size-limit",
    "jsdocs": "jsdoc ./src -r -t ./node_modules/minami -d ./docs/ -R README.md",
    "jsdocs-json": "jsdoc -X ./src -r > docs.json"
  },
  "size-limit": [
    {
      "path": "./lib/fun.js",
      "webpack": "./webpack.config.js"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/astuanax/fun.git"
  },
  "keywords": [
    "webpack",
    "es6",
    "library",
    "universal",
    "umd",
    "commonjs",
    "javascript",
    "monad",
    "functional programming"
  ],
  "author": "Len Dierickx",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/astuanax/fun/issues"
  },
  "homepage": "https://github.com/astuanax/fun",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.5.5",
    "@istanbuljs/nyc-config-babel": "^2.1.1",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^5.2.0",
    "chai": "^4.1.2",
    "create-index": "^2.4.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.14.1",
    "eslint-loader": "^2.1.2",
    "jsdom": "11.11.0",
    "jsdom-global": "3.0.2",
    "minami": "^1.2.3",
    "mocha": "^4.0.1",
    "nyc": "^14.1.1",
    "size-limit": "^1.3.6",
    "standard": "^12.0.1",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "yargs": "^10.0.3"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "dependencies": {}
}
